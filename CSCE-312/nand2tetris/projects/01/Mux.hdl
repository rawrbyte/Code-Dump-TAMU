/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
	Not(in=sel, out=NotSel);
	And(a=a, b=NotSel, out=AndA);
	And(a=sel, b=b, out=AndB);
	Or(a=AndA, b=AndB, out=out);
}